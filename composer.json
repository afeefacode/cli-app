{
    "name": "afeefa/cli-app",
    "version": "0.0.30",
    "type": "library",
    "description": "Convenient wrapper around symfony console",
    "license": "MIT",
    "homepage": "https://github.com/afeefacode/cli-app",
    "authors": [
        {
            "name": "Afeefa Kollektiv",
            "email": "kollektiv@afeefa.de"
        }
    ],
    "require": {
        "php": ">=7.4",
        "afeefa/debug-dump-log": "^0",
        "symfony/console": "^v5.4 || ^v6.1",
        "symfony/process": "^v5.4 || ^v6.1",
        "twig/twig": "^v3.4",
        "webmozart/path-util": "^2.3",
        "wujunze/php-cli-color": "^v2.4"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Afeefa\\Component\\Cli\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Example\\": "example"
        }
    }
}
